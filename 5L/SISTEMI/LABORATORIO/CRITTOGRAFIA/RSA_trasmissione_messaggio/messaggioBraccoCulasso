------------------------- Messaggio da Culasso a Bracco -------------------------
Scooby

S --> 01010011
c --> 01100011
o --> 01101111
o --> 01101111
b --> 01100010
y --> 01111001

2^g >= 143 --> g = 8

01010011 01100011 01101111 01101111 01100010 01111001
	83		 	99      111       111      98      121         

 83 ^ 103 mod 143 =  73 --> 01001001
 99 ^ 103 mod 143 =  44 --> 00101100	
111 ^ 103 mod 143 =  45 --> 00101101		
111 ^ 103 mod 143 =  45 --> 00101101	
 98 ^ 103 mod 143 =  32 --> 00100000
121 ^ 103 mod 143 = 121 --> 01111001		

messaggio cifrato --> 1001001 101100 101101 101101 100000 1111001

 73 ^ Npri mod 143 = 83 
 44 ^ Npri mod 143 = 99  
 45 ^ Npri mod 143 = 111 
 45 ^ Npri mod 143 = 111 
 32 ^ Npri mod 143 = 98  
121 ^ Npri mod 143 = 121  


